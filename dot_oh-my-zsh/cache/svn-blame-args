args=( ${(Q)"${(z)$(<<\EO:args
'(--revision)-r+:arg:' '(-r)--revision=:arg:' '(--verbose)-v' '(-v)--verbose' '(--use-merge-history)-g' '(-g)--use-merge-history' '--incremental' '--xml' '(--extensions)-x+:arg:' '(-x)--extensions=:arg:' '--force' '--username=:arg:' '--password=:arg:' '--no-auth-cache' '--non-interactive' '--force-interactive' '--trust-server-cert' '--trust-server-cert-failures=:failures:_values -s , "certificate failures to ignore" "unknown-ca[unknown authority]" "cn-mismatch[hostname mismatch]" "expired[certificate expired]" "not-yet-valid[certificate not yet valid]" "other[all other failures]"' '--config-dir=:config dir:_directories' '*--config-option=: :_svn_config_options'
EO:args
)}"} )
