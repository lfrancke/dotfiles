args=( ${(Q)"${(z)$(<<\EO:args
'(--revision)-r+:arg:' '(-r)--revision=:arg:' '(--change)-c+:arg:' '(-c)--change=:arg:' '--old=:arg:' '--new=:arg:' '(--non-recursive)-N' '(-N)--non-recursive' '--depth=:operation depth (how far to recurse):(empty files immediates infinity)' '--diff-cmd=:arg:' '--internal-diff' '(--extensions)-x+:arg:' '(-x)--extensions=:arg:' '--no-diff-added' '--no-diff-deleted' '--ignore-properties' '--properties-only' '--show-copies-as-adds' '--notice-ancestry' '--summarize' '*--cl=:change list:_svn_changelists' '*--changelist=:change list:_svn_changelists' '*--cl=:change list:_svn_changelists' '*--changelist=:change list:_svn_changelists' '--force' '--xml' '--git' '--patch-compatible' '--username=:arg:' '--password=:arg:' '--password-from-stdin' '--no-auth-cache' '--non-interactive' '--force-interactive' '--trust-server-cert' '--trust-server-cert-failures=:failures:_values -s , "certificate failures to ignore" "unknown-ca[unknown authority]" "cn-mismatch[hostname mismatch]" "expired[certificate expired]" "not-yet-valid[certificate not yet valid]" "other[all other failures]"' '--config-dir=:config dir:_directories' '*--config-option=: :_svn_config_options'
EO:args
)}"} )
