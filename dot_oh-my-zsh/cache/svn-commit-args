args=( ${(Q)"${(z)$(<<\EO:args
'(--quiet)-q' '(-q)--quiet' '(--non-recursive)-N' '(-N)--non-recursive' '--depth=:operation depth (how far to recurse):(empty files immediates infinity)' '--targets=:arg:' '--no-unlock' '(--message)-m+:arg:' '(-m)--message=:arg:' '(-F --file)-F+:log message file:_files' '(-F --file)--file=:log message file:_files' '(-F --file)-F+:log message file:_files' '(-F --file)--file=:log message file:_files' '--force-log' '--editor-cmd=:arg:' '--encoding=:arg:' '--with-revprop=:arg:' '*--cl=:change list:_svn_changelists' '*--changelist=:change list:_svn_changelists' '*--cl=:change list:_svn_changelists' '*--changelist=:change list:_svn_changelists' '--keep-changelists' '--include-externals' '--username=:arg:' '--password=:arg:' '--no-auth-cache' '--non-interactive' '--force-interactive' '--trust-server-cert' '--trust-server-cert-failures=:failures:_values -s , "certificate failures to ignore" "unknown-ca[unknown authority]" "cn-mismatch[hostname mismatch]" "expired[certificate expired]" "not-yet-valid[certificate not yet valid]" "other[all other failures]"' '--config-dir=:config dir:_directories' '*--config-option=: :_svn_config_options'
EO:args
)}"} )
